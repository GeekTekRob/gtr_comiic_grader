{
  "name": "gtr-comic-grader",
  "version": "1.0.0",
  "description": "AI-powered comic book grading platform using CGC standards",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "setup": "node setup.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "node --watch src/server.js",
    "dev:client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "node src/server.js",
    "test": "jest"
  },
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.3.1",
    "axios": "^1.6.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "@google/generative-ai": "^0.3.0",
    "openai": "^4.24.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0"
  }
}
